{
    "background":{
		"page": "./background.html",
        "persistent": false
		
    },
    "description": "Video Full Web Play",
    "icons": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "64": "./icons/icon64.png",
        "128": "./icons/icon128.png"
    },
    "manifest_version": 2,
    "name": "Full Play",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {

        }
    },
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "css": [],
            "js": [ "./js/curWindow.js"]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "version": "1.0.1.6"
}
